{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "buildCommand": "prisma generate --schema=./prisma/schema.prisma && next build",
  "installCommand": "npm install && prisma generate --schema=./prisma/schema.prisma",
  "outputDirectory": ".next"
}
